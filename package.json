{
  "name": "MERN_boilerplate",
  "version": "1.0.0",
  "private": "true",
  "description": "Boilerplate for Mongo Express React Node",
  "main": "index.js",
  "scripts": {
    "start": "node webapp.js",
    "babel-watch": "babel --presets react src --watch --out-dir static",
    "watch": "webpack --watch",
    "test": "mocha test/**/*-test.js --compilers js:babel-core/register --recursive",
    "mongo": "mkdir mongo-db; mongod --dbpath mongo-db",
    "mongo-win": "md mongo-db & \"C:/Program Files/MongoDb/Server/3.2/bin/mongod.exe\" --dbpath mongo-db"
  },
  "author": "Thorne Brandt",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "history": "^4.3.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-jsdom": "^1.1.0",
    "moment": "^2.15.2",
    "mongodb": "^2.2.11",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-countdown-clock": "^1.0.9",
    "react-dom": "^15.3.2",
    "react-native-timer": "^1.1.2",
    "react-router": "^3.0.0",
    "react-youtube": "^7.2.0",
    "testdom": "^2.0.0",
    "underscore": "^1.8.3",
    "webpack": "^1.13.3",
    "whatwg-fetch": "^1.0.0"
  }
}
